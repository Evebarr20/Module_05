    "# import python file\n",
    "import file\n",
    "\n",
    "# open file to read\n",
    "\n",
    "fhandle = open(file.file_choose(), \"r\", encoding=\"utf8\")\n",
    "\n",
    "# print out first line of text file\n",
    "print(fhandle.readline())\n",
    "\n",
    "fhandle.close()"
